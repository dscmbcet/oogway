    "googleapis": "^87.0.0",
    "juice": "^8.0.0",
    "node-fetch": "^2.6.6",
    "nodemailer": "^6.7.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cron": "^1.7.3",
    "@types/uuid": "^8.3.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
