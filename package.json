{
  "name": "master_oogway",
  "version": "1.5.0",
  "description": "Official DSC Bot for DSC Discord Server",
  "main": "discord.js",
  "scripts": {
    "debug": "nodemon discord.js",
    "start": "forever --minUptime 1000 --spinSleepTime 1000 discord.js",
    "generate-commands.md": "node ./utils/generate_command_readme.js",
    "get-logs-heroku-app-only": "heroku logs --tail -a dsc-professor-bot --source app",
    "get-logs-heroku": "heroku logs --tail -a dsc-professor-bot",
    "connect-bash-heroku": "heroku run bash --app dsc-professor-bot",
    "manual-deploy": "git push heroku main"
  },
  "author": "DSC MBCET",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "forever": "^4.0.1",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}