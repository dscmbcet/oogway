{
  "name": "master_oogway",
  "version": "2.2.0",
  "description": "Official GDSC Bot for GDSC Discord Server",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "nodemon index.js",
    "update-command-readme": "node ./utils/update-command-readme.js",
    "start": "node index.js",
    "get-logs-heroku-app-only": "heroku logs --tail -a dsc-oogway-bot --source app",
